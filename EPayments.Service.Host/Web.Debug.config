<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="EPaymentsContext"
      connectionString="Data Source=.;Initial Catalog=EPaymentsLocalTest;Integrated Security=True;MultipleActiveResultSets=True"
      xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="EPaymentsLogs"
      connectionString="Data Source=.\;Initial Catalog=EPaymentsLogs1LocalTest;Integrated Security=True;MultipleActiveResultSets=True"
      xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
  </connectionStrings>
  <appSettings>
    <add key="EPayments.EventRegister:ValidateSslCert" value="false"  xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
  </appSettings>
</configuration>